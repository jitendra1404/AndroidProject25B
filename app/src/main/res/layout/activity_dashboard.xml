<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="DashboardActivity">


    <ImageView
        android:id="@+id/btnabout"
        android:layout_width="87dp"
        android:layout_height="52dp"
       android:src="@drawable/about_3697"
        app:layout_constraintBottom_toBottomOf="@+id/btnreview"
        app:layout_constraintStart_toEndOf="@+id/btnreview" />

    <ImageView
        android:id="@+id/btnreview"
        android:layout_width="90dp"
        android:layout_height="52dp"
        android:layout_marginStart="4dp"
       android:src="@drawable/review"
        app:layout_constraintBottom_toBottomOf="@+id/btnappointment"
        app:layout_constraintStart_toEndOf="@+id/btnappointment" />

    <ImageView
        android:id="@+id/btnappointment"
        android:layout_width="110dp"
        android:layout_height="52dp"
        android:textAlignment="center"
        android:src="@drawable/appointment_icon_179680"
        app:layout_constraintBottom_toBottomOf="@+id/btndashboard"
        app:layout_constraintStart_toEndOf="@+id/btndashboard" />

    <ImageView
        android:id="@+id/btndashboard"
        android:layout_width="110dp"
        android:layout_height="52dp"
        android:src="@drawable/ic_dashboard"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:id="@+id/linearcontainer"
        android:layout_width="match_parent"
        android:layout_height="560dp"
        android:layout_marginTop="128dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnappointment" />


</androidx.constraintlayout.widget.ConstraintLayout>







   


